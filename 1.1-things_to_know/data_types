// User function Template for C++

class Solution {
  public:
    int dataTypeSize(string str) {
        // your code here
        int digit;
    
       if (str=="Character")
       digit=1;
       else if(str=="Double" || str=="Long")
       digit=8;
       else digit=4;
       return digit;
    }
};

//{ Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        string str;
        cin >> str;
        Solution ob;
        cout << ob.dataTypeSize(str);
        cout << "\n";
    }
    return 0;
}

